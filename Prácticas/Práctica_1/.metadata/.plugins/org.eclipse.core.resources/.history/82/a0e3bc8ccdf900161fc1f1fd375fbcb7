 */
public abstract class MeteoSimulador {
	
	private ArrayList<Observador> simuladoresConcretos;	
	
	public MeteoSimulador(){}
	
	public void incluirObservador(Observador simulador){
		simuladoresConcretos.add(simulador);
	}
	
	public void definirObservador(){
		
	}

}
